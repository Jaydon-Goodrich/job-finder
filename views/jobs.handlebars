{{!-- Page for placing all the jobs pulled form linked in --}}
<h2>Jobs page!</h2>

{{#each data}}
<div class="card" style="width: 18rem;">
    <h4 class="card-title">{{this.title}}</h2>
    <h6 class="card-subtitle mb-2">{{this.company}}<span class="text-muted city-text">   {{this.place}}</span></h6>

    <a href="{{this.link}}" target="_blank" class="card-link">Click Here to Apply</a>
</div>

{{/each}}


